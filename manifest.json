{
  "name": "Image Scatter Plot",
  "organization": "Custom Viz Studio",
  "description": "Scatter plot visualization with creative images and size scaling for advertising data analysis.",
  "logoUrl": "https://via.placeholder.com/128x128/4285f4/ffffff?text=IMG",
  "organizationUrl": "https://example.com",
  "supportUrl": "https://example.com/support",
  "privacyUrl": "https://example.com/privacy",
  "termsOfServiceUrl": "https://example.com/terms",
  "devMode": true,
  "components": [
    {
      "id": "image_scatter_plot",
      "name": "Image Scatter Plot",
      "iconUrl": "https://via.placeholder.com/64x64/34a853/ffffff?text=SP",
      "resource": {
        "js": "main.js",
        "css": "style.css"
      },
      "data": [
        {
          "id": "DEFAULT",
          "label": "Default",
          "elements": [
            {
              "id": "creativeId",
              "label": "Creative ID",
              "type": "DIMENSION",
              "options": {
                "min": 1,
                "max": 1
              }
            },
            {
              "id": "imageUrl",
              "label": "Image URL",
              "type": "DIMENSION", 
              "options": {
                "min": 1,
                "max": 1
              }
            },
            {
              "id": "xMetric",
              "label": "X-Axis Metric",
              "type": "METRIC",
              "options": {
                "min": 1,
                "max": 1
              }
            },
            {
              "id": "yMetric",
              "label": "Y-Axis Metric", 
              "type": "METRIC",
              "options": {
                "min": 1,
                "max": 1
              }
            },
            {
              "id": "sizeMetric",
              "label": "Size Metric",
              "type": "METRIC",
              "options": {
                "min": 0,
                "max": 1
              }
            }
          ]
        }
      ],
      "style": [
        {
          "id": "xLabel",
          "label": "X-axis Label",
          "type": "TEXTINPUT",
          "defaultValue": "CTR (%)"
        },
        {
          "id": "yLabel",
          "label": "Y-axis Label", 
          "type": "TEXTINPUT",
          "defaultValue": "CVR (%)"
        },
        {
          "id": "minImageSize",
          "label": "Minimum Image Size (px)",
          "type": "TEXTINPUT",
          "defaultValue": "20"
        },
        {
          "id": "maxImageSize",
          "label": "Maximum Image Size (px)",
          "type": "TEXTINPUT", 
          "defaultValue": "80"
        },
        {
          "id": "showGridLines",
          "label": "Show Grid Lines",
          "type": "CHECKBOX",
          "defaultValue": true
        },
        {
          "id": "showTooltips",
          "label": "Show Tooltips",
          "type": "CHECKBOX",
          "defaultValue": true
        }
      ]
    }
  ]
}
    }
  ]
}
